################################################################################
# N8N DEVELOPMENT ENVIRONMENT
# Copy this file to .env and adjust values as needed
################################################################################

########################
# GENERAL
########################
TZ=America/Sao_Paulo
GENERIC_TIMEZONE=America/Sao_Paulo

########################
# N8N - PostgreSQL
########################
N8N_POSTGRES_HOST=n8n-postgresql
N8N_POSTGRES_PORT=5432
N8N_POSTGRES_USER=postgres
# command: openssl rand -base64 32
N8N_POSTGRES_PASSWORD=your-n8n-postgres-password
N8N_POSTGRES_DB=n8n

########################
# N8N - Redis
########################
N8N_REDIS_HOST=n8n-redis
N8N_REDIS_PORT=6379
# command: openssl rand -base64 32
N8N_REDIS_PASSWORD=your-n8n-redis-password

########################
# N8N - Application
########################
# command: uuidgen or openssl rand -hex 16
N8N_ENCRYPTION_KEY=your-n8n-encryption-key
N8N_HOST=localhost
N8N_PORT=5678
N8N_EDITOR_BASE_URL=http://localhost:5678
N8N_WEBHOOK_URL=http://localhost:5678

########################
# EVOLUTION API
########################
EVOLUTION_PORT=8585
EVOLUTION_POSTGRES_DB=evolution
# command: openssl rand -hex 32
EVOLUTION_API_KEY=your-evolution-api-key

